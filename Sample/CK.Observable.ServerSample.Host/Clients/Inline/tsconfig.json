{
  "compilerOptions": {
    "target": "es2022",
    "moduleResolution": "node",
    "lib": [
      "es2022",
      "dom"
    ],
    "module": "ES2015",
    "outDir": "./dist",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "paths": {
      "@local/ck-gen": [
        "./ck-gen"
      ],
      "@local/ck-gen/*": [
        "./ck-gen/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}
